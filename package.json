{
  "name": "erc20-nft-tokens-bundler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prepare": "husky install",
    "postinstall": "husky install",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|tsx|vue)\"",
    "format": "prettier --write --ignore-path .gitignore \"src/**/*.+(ts|tsx|vue)\"",
    "check-format": "prettier --list-different --ignore-path .gitignore \"src/**/*.+(ts|tsx|vue)\"",
    "types-check": "tsc --noEmit",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.26.1",
    "core-js": "^3.6.5",
    "ethers": "^5.6.1",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "pinst": "^3.0.0",
    "prettier": "^2.6.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5"
  }
}
